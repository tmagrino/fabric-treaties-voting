#!/bin/bash

APP_HOME="$(dirname $0)/.."

source "${APP_HOME}/config.sh"

PATH="${FABRIC}/bin:${PATH}"

CP="${APP_HOME}/classes"
CP="${CP}:${FABRIC}/src/lib/sif/classes"
CP="${CP}:${FABRIC}/lib/fabric-sif.jar"

export FABRIC_HOME="${APP_HOME}"

fab --jvm-cp "${CP}" --app-home "${FABRIC_HOME}" --name "$@"

